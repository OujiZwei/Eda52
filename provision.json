{
  "android.app.extra.PROVISIONING_DEVICE_ADMIN_COMPONENT_NAME": "com.afwsamples.testdpc/.DeviceAdminReceiver",
  "android.app.extra.PROVISIONING_DEVICE_ADMIN_PACKAGE_NAME": "com.afwsamples.testdpc",
  "android.app.extra.PROVISIONING_MODE": "DEVICE_OWNER",
  "android.app.extra.PROVISIONING_LEAVE_ALL_SYSTEM_APPS_ENABLED": false,

  "policy": {
    "defaultPermissionPolicy": "GRANT",

    "statusBarDisabled": true,
    "keyguardDisabled": true,
    "maximumTimeToLock": 0,
    "stayOnWhilePluggedIn": 3,

    "kioskCustomLauncherEnabled": true,

    "lockTaskPackages": [
      "com.afwsamples.testdpc",
      "uk.nktnet.webviewkiosk"
    ],

    "persistentPreferredActivities": [
      {
        "actions": ["android.intent.action.MAIN"],
        "categories": ["android.intent.category.HOME"],
        "receiverActivity": "com.afwsamples.testdpc/.LauncherActivity"
      }
    ],

    "managedConfiguration": {
      "kioskUrl": "https://www.lowcosttinaytoner.com/panel",
      "fullscreen": true,
      "allowNewWindows": false,
      "allowBackButton": false,
      "allowedDomains": [
        "lowcosttinaytoner.com"
      ]
    }
  }
}
